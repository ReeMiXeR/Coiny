<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="200dp"
    android:layout_height="100dp"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:background="@color/colorPrimary">

    <TextView
        android:id="@+id/currency_pair_from_to"
        app:layout_constraintTop_toBottomOf="@id/currency_pair_martket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="BTC/USD"/>

    <TextView
        android:id="@+id/currency_pair_price"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="$ 6,297.99"/>

    <TextView
        android:id="@+id/currency_pair_change_day"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:text="$ -11.12"/>
    <TextView
        android:id="@+id/currency_pair_martket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="CryptoCompare "/>

</android.support.constraint.ConstraintLayout>

